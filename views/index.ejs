<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
</head>

<body>
    <h1>
        <%= title %>
    </h1>

    <!-- Mostrar los posts aquÃ­ -->

    <% posts.forEach(function(posts) { %>
        <div>
            <fieldset>
            <ul>
                <h3>ID: <%=posts.id %> </h3>
                <li>Titulo: <%=posts.title %></li>
                <li>Descripcion: <%= posts.description %></li>
                <li>Autor: <%=posts.author %></li>
            </ul>
        </fieldset>
        </div>
        <br>
        <% }) %>

</body>

</html>